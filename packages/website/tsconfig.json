{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "preserve",
    "esModuleInterop": true
  },
  "include": ["src", "typings"]
}
